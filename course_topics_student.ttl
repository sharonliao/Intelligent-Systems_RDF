focudata:GEOG625 a focu:Course ;
    focu:offeredAt focudata:cu ;
    dc:description "With written permission of the graduate program director, a student studies a particular field or topic relating to geography, urban or environment studies. A detailed outline of the proposed study, approved by a study supervisor is required." ;
    dc:identifier "625" ;
    dc:subject "GEOG" ;
    rdfs:seeAlso <https://www.concordia.ca/academics/graduate/calendar/current/fasc/geog-msc.html> ;
