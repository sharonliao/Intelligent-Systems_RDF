    focu:offeredAt focudata:cu ;
    dc:description "Prerequisite: Honours status. This course provides the honours candidate with the opportunity to prepare an extensive research project on a topic to be chosen by the candidate with the approval of a supervising member of the faculty of the Spanish section." ;
    dc:identifier "490" ;
    dc:subject "SPAN" ;
    rdfs:seeAlso <https://www.concordia.ca/academics/undergraduate/calendar/current/sec31/31-060.html> ;
