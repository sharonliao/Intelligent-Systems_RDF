focudata:MATH649 a focu:Course ;
    focu:offeredAt focudata:cu ;
    dc:description "This course examines cognitive processes, tools and strategies involved in solving mathematical problems." ;
    dc:identifier "649" ;
    dc:subject "MATH" ;
    rdfs:seeAlso <https://www.concordia.ca/academics/graduate/calendar/current/fasc/mast-mtm.html> ;
