    dc:identifier "332" ;
    dc:subject "POLI" ;
    rdfs:seeAlso <https://www.concordia.ca/academics/undergraduate/calendar/current/sec31/31-240.html> ;
    foaf:name "Theories of International Relations " .

focudata:POLI334 a focu:Course ;
