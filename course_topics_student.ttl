    dc:identifier "334" ;
    dc:subject "POLI" ;
    rdfs:seeAlso <https://www.concordia.ca/academics/undergraduate/calendar/current/sec31/31-240.html> ;
    foaf:name "Political Participation in Canada " .

focudata:POLI335 a focu:Course ;
