    dc:identifier "349" ;
    dc:subject "POLI" ;
    rdfs:seeAlso <https://www.concordia.ca/academics/undergraduate/calendar/current/sec31/31-240.html> ;
    foaf:name "Political and Social Theory and the City " .

focudata:POLI351 a focu:Course ;
    focu:hasTopic focudata:Canadian_Federalism,
        focudata:dualism ;
    focu:offeredAt focudata:cu ;
