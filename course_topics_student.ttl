focudata:MHIS298 a focu:Course ;
    focu:offeredAt focudata:cu ;
    dc:description "Prerequisite: Enrolment in a program in the Department of Music or written permission of the Department of Music. A study of a selected area not available in other courses in music history." ;
    dc:identifier "298" ;
    dc:subject "MHIS" ;
    rdfs:seeAlso <https://www.concordia.ca/academics/undergraduate/calendar/current/sec81/81-100.html> ;
